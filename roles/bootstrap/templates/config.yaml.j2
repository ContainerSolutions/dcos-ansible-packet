---
bootstrap_url: http://{{ ansible_bond0_0.ipv4.address }}:{{ bootstrap_server_port }}
cluster_name: 'my-cluster'
exhibitor_storage_backend: static
ip_detect_filename: /genconf/ip-detect
master_discovery: static
master_list:
- {{ hostvars[groups['masters'][0]]['ansible_bond0_0']['ipv4']['address'] }}
resolvers:
{% for dns_server in dns_servers %}
  - {{ dns_server }}
{% endfor %}
use_proxy: 'false'
